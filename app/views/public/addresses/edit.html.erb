<h4>配送先編集</h4>
<%= form_with model: @addresses, url: address_path do |f| %>
<h5>郵便番号</h5>
  <%= f.text_field :postal_code, class: "form-group" %>
<h5>住所</h5>
  <%= f.text_field :address, class: "form-group" %>
<h5>宛名</h5>
  <%= f.text_field :name, class: "form-group" %>
  <%= f.submit "変更内容を保存" %>
<% end %>